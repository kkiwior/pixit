{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Redis": {
    "AbortOnConnectionFail": true,
    "Password": "",
    "AllowAdmin": true,
    "Ssl": false,
    "ConnectTimeout": 300,
    "ConnectRetry": 2,
    "Hosts": [
      {
        "Host": "host.docker.internal",
        "Port": "6379"
      }
    ],
    "Database": 0,
    "MaxValueLength": 4096,
    "PoolSize": 5,
    "KeyPrefix": "room_"
  }
}
